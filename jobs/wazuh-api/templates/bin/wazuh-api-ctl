#!/bin/bash

set -e # exit immediately if a simple command exits with a non-zero status
set -u # report the usage of uninitialized variables

chown -Rf ossec:ossec /var/vcap/store/ossec-server/logs/

exec /var/vcap/packages/nodejs/node/bin/node /var/vcap/packages/wazuh-api/api/app.js >> $LOG_DIR/$JOB_NAME.execd.log 2>&1
